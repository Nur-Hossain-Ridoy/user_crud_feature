{
  "name": "node-usermanagement",
  "version": "1.0.0",
  "description": "A simple UserManagement System",
  "main": "./src/index.ts",
  "typesVersions": {
    ">=4.9": { "*": ["ts4.9/*"] }
  },
  "scripts": {
    "prestart:dev": "npm run build",
    "start:dev": "concurrently \"tsc -w\" \"nodemon ./dist/index.js\"",
    "build": "rimraf dist && npx tsc",
    "prestart": "npm run build",
    "start": "node dist/index.js"
  },
  "author": "NH Ridoy",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-handlebars": "^6.0.6",
    "mysql": "^2.18.1",
    "rimraf": "^4.1.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
